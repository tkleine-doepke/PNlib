// name: PNlib.PN.Examples.ExtTest.TDStruncatednormal
// keywords: PNlib, continuous
// status: correct

loadFile("../../PNlib/package.mo"); getErrorString();
simulate(PNlib.PN.Examples.ExtTest.TDStruncatednormal); getErrorString();

vars := {
  "P1.t",
  "P2.t",
  "T1.active",
  "T1.putTime"
};

(success, failVars) := OpenModelica.Scripting.diffSimulationResults("PNlib.PN.Examples.ExtTest.TDStruncatednormal_res.mat",
  "../../ReferenceFiles/PNlib.PN.Examples.ExtTest.TDStruncatednormal.mat",
  "PNlib.PN.Examples.ExtTest.TDStruncatednormal_diff", vars=vars); getErrorString();

if success then
  print("Files Equal!");
else
  filterSimulationResults("PNlib.PN.Examples.ExtTest.TDStruncatednormal_res.mat", "../../ReferenceFiles/PNlib.PN.Examples.ExtTest.TDStruncatednormal_minimal.mat", vars, 0); getErrorString();
end if;

