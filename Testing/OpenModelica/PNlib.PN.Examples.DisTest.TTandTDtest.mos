// name: PNlib.PN.Examples.DisTest.TTandTDtest
// keywords: PNlib, discrete
// status: correct

loadFile("../../PNlib/package.mo"); getErrorString();
simulate(PNlib.PN.Examples.DisTest.TTandTDtest); getErrorString();

vars := {
  "P2.t",
  "P3.t",
  "P4.t",
  "T3.active",
  "T4.active"
};

(success, failVars) := OpenModelica.Scripting.diffSimulationResults("PNlib.PN.Examples.DisTest.TTandTDtest_res.mat",
  "../../ReferenceFiles/PNlib.PN.Examples.DisTest.TTandTDtest.mat",
  "PNlib.PN.Examples.DisTest.TTandTDtest_diff", vars=vars); getErrorString();

if success then
  print("Files Equal!");
else
  filterSimulationResults("PNlib.PN.Examples.DisTest.TTandTDtest_res.mat", "../../ReferenceFiles/PNlib.PN.Examples.DisTest.TTandTDtest_minimal.mat", vars, 0); getErrorString();
end if;

